extends ./layout.jade

block content
    header
        .header-content
            .header-content-inner
              .row
                    each post in posts
                        .post
                        .col-lg-4.text-center.portfolio-box
                          h2.sectionheading #{post.title}
                          hr.light
                          a(href='/studentHome/' + post.title).btn.btn-default Go!